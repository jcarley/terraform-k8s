kops create cluster \
  --cloud=aws \
  --name=kubernetes.finishfirstsoftware.com \
  --dns-zone=kubernetes.finishfirstsoftware.com \
  --state=s3://com.finishfirstsoftware.deploy/k8s/state-store \
  --api-loadbalancer-type=public \
  --zones="us-west-2b" \
  --topology=public \
  --networking=weave \
  --node-count=2 \
  --node-size=t2.micro \
  --master-size=t2.micro \
  --out=. \
  --target=terraform
